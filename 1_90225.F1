      Se (Cur_E012Fam.Achou)
         Inicio
         VCtaCre = Cur_E012Fam.CtaFcr;
         Fim;
      Cur_E012Fam.FecharCursor();
      Fim;

/* Se não achou Conta Contabil na Família - Busca no cadastro do Produto */

   Se ((vCtaCre = 0) e (vCodpro <> ' '))
      Inicio

        Cur_E075Pro.Sql "Select CtaFcr  \
                           From E075Pro \
                          Where CodEmp = :vCodEmp \
                            And Codpro = :vCodpro";
        Cur_E075Pro.AbrirCursor();
        Se (Cur_E075Pro.Achou)
          Inicio
            vCtaFcr = Cur_E075Pro.CtaFcr;
          Fim;

      Fim;

VCtaCre1 = VCtaCre;
