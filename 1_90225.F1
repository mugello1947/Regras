/* Se não achou Conta Contabil - Busca no Cadastro da Familia*/

   Se ((vCtaCre = 0) e (VCodpro <> ' '))
      Inicio
      Cur_E012Fam.Sql "Select CtaFcr  \
                       From E012Fam \
                      Where CodEmp = :vCodEmp \
                        And CodFam = :vCodFam";
